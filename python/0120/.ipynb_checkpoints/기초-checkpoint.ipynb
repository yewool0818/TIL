      "10\n"
     ]
    }
   ],
   "source": [
    "# 1. 변수 초기화\n",
    "# 대상\n",
    "numbers = range(1, 21)\n",
    "# 바를 정\n",
    "count = 0\n",
    "\n",
    "# 반복\n",
    "for number in numbers:\n",
    "    # 2로 나눈 나머지 0, 1\n",
    "    # 짝수면, 0 => 0은 boolean => False\n",
    "    # 홀수면, 1 => 1은 boolean => True\n",
    "    if number % 2:\n",
    "        count += 1\n",
    "        \n",
    "print(count)"
   ]
  }
